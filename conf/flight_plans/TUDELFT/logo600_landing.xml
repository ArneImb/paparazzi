<!DOCTYPE flight_plan SYSTEM "../flight_plan.dtd">
<flight_plan alt="180" ground_alt="150" lat0="52.1707480" lon0="4.4092508" max_dist_from_home="500" name="Logo600_landing" security_height="2">
  <header>
#include "autopilot.h"
  </header>
  <waypoints>
    <waypoint name="HOME" x="0.0" y="0.0"/>
    <waypoint name="STDBY" x="-2.0" y="-5.0"/>
    <waypoint name="CAM" x="-20" y="-50" height="2."/>
    <waypoint name="TD" x="5.6" y="-10.9"/>
    <waypoint name="JOE" x="0.0" y="-50.0"/>
    <waypoint name="CIRCLE" x="0.0" y="-50.0"/>
    <waypoint name="LANDING" x="10.0" y="10.0"/>
  </waypoints>
  <blocks>
    <block name="Wait GPS">
      <call fun="NavKillThrottle()"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block name="Geo init">
      <while cond="LessThan(NavBlockTime(), 10)"/>
      <!--call fun="NavSetGroundReferenceHere()"/ -->
      <call fun="NavSetAltitudeReferenceHere()"/>
    </block>
    <block name="Holding point">
      <call fun="setZeroHeight()" loop="false"/>
      <call fun="enableLandingspotSearch(false)" loop="false"/>
      <call fun="enableKalamosDescent(false)" loop="false"/>
      <call fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" vmode="throttle" until="FALSE"/>
    </block>
    <block name="Standby" strip_button="Standby" strip_icon="home.png">
      <stay wp="STDBY"/>
    </block>
    <block name="Stay_td">
      <call fun="enableKalamosDescent(false)" loop="false"/>
      <stay wp="TD"/>
    </block>
    <block name="Circle_joe" pre_call="nav_set_heading_towards(POS_FLOAT_OF_BFP(navigation_target.x),POS_FLOAT_OF_BFP(navigation_target.y))">
      <call fun="enableLandingspotSearch(true)" loop="false"/>
      <call fun="enableKalamosDescent(false)" loop="false"/>
      <circle wp="CIRCLE" radius="75" until="block_time>180"/>
    </block>
    <block name="HoldPositionOnCircle">
      <call fun="enableLandingspotSearch(false)" loop="false"/>
      <call fun="NavSetWaypointHere(WP_TD)" loop="false"/>
      <stay wp="TD"/>
    </block>
    <block name="Goto_landing">
      <stay wp="LANDING" until="block_time>10"/>
    </block>
    <block name="Do_landing">
      <call fun="enableKalamosDescent(true)" loop="false"/>
      <stay wp="LANDING"/>
    </block>
  </blocks>
</flight_plan>
